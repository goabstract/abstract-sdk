{
  "name": "abstract-sdk",
  "version": "1.8.0",
  "description": "Universal JavaScript bindings for the Abstract API and CLI",
  "keywords": [
    "abstract",
    "design",
    "version control"
  ],
  "homepage": "https://sdk.goabstract.com/",
  "bugs": {
    "email": "support@abstract.com",
    "url": "https://github.com/goabstract/abstract-sdk/issues"
  },
  "license": "MIT",
  "author": "Abstract <support@abstract.com> (http://abstract.com)",
  "files": [
    ".flowconfig",
    "/definitions",
    "/lib",
    "/scripts",
    "abstract-sdk.d.ts"
  ],
  "main": "lib/index.js",
  "types": "abstract-sdk.d.ts",
  "browser": {
    "child_process": false,
    "fs": false
  },
  "repository": "github:goabstract/abstract-sdk",
  "scripts": {
    "prepare": "./scripts/install-types.sh",
    "commitlint": "commitlint --edit",
    "lint": "eslint . --ignore-path .gitignore",
    "test": "jest --coverage",
    "prebuild": "rm -rf lib && flow-copy-source -i *.test.js -i endpoints/*.test.js -i testing.js src lib",
    "build": "babel src -d lib --ignore **/*.test.js",
    "prepublish": "yarn build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@elasticprojects/eslint-config-abstract": "^4.1.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.1.0",
    "eslint": "^4.8.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.101.0",
    "flow-copy-source": "^2.0.3",
    "jest": "^24.1.0",
    "nock": "^10.0.6",
    "prettier": "^1.14.2",
    "standard-version": "^6.0.1"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.3.4",
    "cross-fetch": "^3.0.1",
    "debug": "^4.0.1",
    "flow-typed": "^2.5.1",
    "query-string": "^6.1.0",
    "uuid": "^3.3.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "@elasticprojects/eslint-config-abstract",
    "env": {
      "jest": true
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-flow",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "targets": {
            "node": 8
          },
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
